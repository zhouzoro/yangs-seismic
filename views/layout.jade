doctype html
html
	head
		title Yangs Seismic Lab
		meta(charset="utf-8")
		meta(name="viewport" content="width=device-width, initial-scale=1")
		link(rel='stylesheet' href='https://cdn.jsdelivr.net/fontawesome/4.5.0/css/font-awesome.min.css')
		link(rel='stylesheet' href='/stylesheets/bootstrap.min.css')
		link(rel='stylesheet' href='/semantic/semantic.min.css')
		link(rel='stylesheet', href='/stylesheets/style.min.css')
		link(rel='stylesheet' href='/stylesheets/person.min.css')
	body
		#vertical-nav
			.first-class
				a.nav-item(href='#about')
					.icon-holder
						i.icon.radio
					span 简介
				a.nav-item(href='#projects')
					.icon-holder
						i.icon.radio.selected
					span 项目
				a.nav-item(href='#people')
					.icon-holder
						i.icon.radio.selected
					span 人员
				a.nav-item(href='#people')
					.icon-holder
						i.icon.radio.selected
					span 活动
				a.nav-item(href='#people')
					.icon-holder
						i.icon.radio
					span FAQ
		#body.col-md-12.col-lg-8.col-lg-offset-2
			include header
			//.ui.steps
				.step
					a.content(href='#') Home
				.step
					a.content(href='#') People
				.step
					a.content(href='#') Richard Hendriks
			//#site-path.ui.breadcrumb
				a.section Home
				.divider /
				a.section People
				.divider /
				a.section.active Richard Hendriks
			#main.container-fluid
				#main-content
					//.col-xs-12.col-md-8.col-lg-9
					block content
				//#news.col-xs-12.col-md-4.col-lg-3
					label news links
					ol.ui.list
						li
							a.item news link1
						li
							a.item news link2
						li
							a.item news link3
			include footer
		block scripts
			script(src='//code.jquery.com/jquery-2.1.4.min.js')
			script(src='https://cdn.jsdelivr.net/lodash/3.10.1/lodash.min.js')
			script(src='https://cdn.jsdelivr.net/jquery.ui/1.11.4/jquery-ui.min.js')
			script(src='https://cdn.jsdelivr.net/semantic-ui/2.1.8/semantic.min.js')
			script(src='https://cdn.jsdelivr.net/js-cookie/2.1.0/js.cookie.js')
			script(src='https://cdn.jsdelivr.net/vue/1.0.15/vue.min.js')
